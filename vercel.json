{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/"
        ]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "src/index.js"
    },
    {
      "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/index.html"
        }
      ]
    }
  ]
}